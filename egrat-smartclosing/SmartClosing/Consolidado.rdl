    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>contabil_consolidado</DataSourceName>
        <CommandText>SELECT
  ledger.id
  ,ledger.debit
  ,ledger.credit
  ,ledger.description
  ,ledger.amount
  ,ledger.post_date
  ,ledger.entry_date
 ,accounts.name
,accounts.Account_Group
FROM
  ledger
INNER JOIN
accounts
ON ledger.credit = accounts.id</CommandText>
